{
  "name": "fullvue",
  "version": "1.0.0",
  "description": "Full-stack vue test app",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "server": "nodemon",
    "client": "cd client && npm run serve",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && cd client && npm run build-app",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build-app --prefix client"
  },
  "author": "Pipirok",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.1.1",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.12",
    "prisma": "^3.1.1"
  },
  "type": "module",
  "engines": {
    "node": "16.x"
  }
}
